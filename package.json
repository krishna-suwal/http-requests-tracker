    "docs:write-heading-ids": "docusaurus write-heading-ids",
    "api-docs:start": "webpack-dev-server --mode development --hot --open --config ./webpack/webpack.config.api-docs.js",
    "api-docs:build": "webpack --mode production --config ./webpack/webpack.config.api-docs.js"
  },
  "workspaces": {
    "packages": [
      "docs/**",
      "docs",
      "docs-src",
      "other-project"
